	<parameter name="codigo" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[select single.exemplar.registro as codigo, single.area.nome as area, single.livro.serie as serie
from single.emprestimo 
inner join single.exemplar
on single.emprestimo.exemplar = single.exemplar.registro
inner join single.livro 
on single.livro.registro = single.exemplar.livro
inner join single.area
on single.area.codigo = single.livro.area
where single.emprestimo.users = $P{codigo}
and single.emprestimo.status = true
and single.emprestimo.data_hora >=  current_date ]]>
	</queryString>
